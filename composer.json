{
    "name": "hexbatch/hbc-thangs",
    "type": "hexbatch-library",
    "description": "The thangs data structure and api to work with it",
    "keywords": ["laravel","core-api", "api","things","hexbatch"],
    "homepage": "https://codeberg.org/Hexbatch/hbc-thangs",
    "authors": [
        {
            "name": "Will Woodlief",
            "homepage": "https://badgreenwolf.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://codeberg.org/Hexbatch/hbc-thangs/issues"
    },
    "license": "Apache-2.0",
    "version": "0.0.1",

    "require": {
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-pgsql": "*",
        "ext-dom": "*",
        "php": "^8.3",
        "bluem/tree": "^3.2",
        "lorisleiva/laravel-actions": "^2.9",
        "spatie/laravel-data": "^4.18",
        "spatie/laravel-package-tools": "^1.19",
        "staudenmeir/laravel-cte": "^1.0",
        "tormjens/eventy": "^0.9.3",
        "zircote/swagger-php": "^5.0"
    },

    "autoload": {
        "psr-4": {
            "Hexbatch\\Thangs\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hexbatch\\Thangs\\HexbatchThangsProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
